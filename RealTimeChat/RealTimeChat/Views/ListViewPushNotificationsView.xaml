<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RealTimeChat.Views.ListViewPushNotificationsView">
        
        <ListView x:Name="MyListView" RowHeight="50" Margin="0,10,0,0">
            <ListView.ItemTemplate>
                <DataTemplate>

                    <ViewCell Height="100">
                        <StackLayout Orientation="Vertical" Margin="5" >
                            <Label Text="{Binding Title}"/>
                            <Label Text="{Binding MessageOwner}" TextColor="Gray"/>
                        </StackLayout>
                    </ViewCell>

                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
        
    
</ContentPage>